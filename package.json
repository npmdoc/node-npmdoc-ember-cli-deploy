    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-ember-cli-deploy"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
