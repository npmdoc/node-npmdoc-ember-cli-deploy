    "keywords": [
        "documentation",
        "ember-cli-deploy"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_ember_cli_deploy.js",
    "name": "npmdoc-ember-cli-deploy",
    "nameAlias": "npmdoc_ember_cli_deploy",
